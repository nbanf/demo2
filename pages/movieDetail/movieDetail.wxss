/* pages/movieDetail/movieDetail.wxss */
.movieDetail_wrap{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20rpx;
}
.movieDetail_picture{
  width: 70%;
  height: 700rpx;
}
.movieDetail_title{
  font-size: 40rpx;
  font-weight: bold;
  margin: 10rpx 0 ;
}
.movieDetail_details{
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
}
.movieDetail_see{
  width: 70%;
}
.movieDetail_see button{
  height: 80rpx;
  background: greenyellow;
  line-height: 80rpx;
  margin-top: 50rpx;
  color: #fff;
}
